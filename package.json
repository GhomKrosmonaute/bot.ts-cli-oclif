{
  "name": "create-bot.ts",
  "version": "1",
  "description": "CLI for make Discord bots in TypeScript quickly with discord.js",
  "author": "GhomKrosmonaute @GhomKrosmonaute",
  "type": "commonjs",
  "bin": {
    "create-bot.ts": "./bin/run",
    "make-bot.ts": "./bin/run",
    "create-bot": "./bin/run",
    "make-bot": "./bin/run",
    "create": "./bin/run",
    "make": "./bin/run",
    "bot": "./bin/run"
  },
  "homepage": "https://github.com/GhomKrosmonaute/create-bot.ts",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "GhomKrosmonaute/create-bot.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "boxen": "^6.2.1",
    "discord.js": "^13.7.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "is-valid-package-name": "^1.0.0",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.1",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/string-similarity": "^4.0.0",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.6.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "create-bot.ts",
    "dirname": "create-bot.ts",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/GhomKrosmonaute/create-bot.ts/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
